{
  "manifest_version": 3,
  "name": "YouTube Redirection",
  "version": "1.0",
  "description": "Adds a button on YouTube videos that shows an alert on click.",
  "permissions": [],
  "icons": {
    "16": "images/image.png",
    "48": "images/image.png",
    "128": "images/image.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["content.js"]
    }
  ]
}
